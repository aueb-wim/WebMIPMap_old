<?xml version="1.0" encoding="UTF-8"?>
<mappingtask>
  <config>
    <rewriteSubsumptions>true</rewriteSubsumptions>
    <rewriteCoverages>true</rewriteCoverages>
    <rewriteSelfJoins>true</rewriteSelfJoins>
    <rewriteEGDs>false</rewriteEGDs>
    <sortStrategy>-1</sortStrategy>
    <skolemTableStrategy>-1</skolemTableStrategy>
    <useLocalSkolem>false</useLocalSkolem>
  </config>
  <source>
    <type>CSV</type>
    <csv>
      <csv-db-name>SourceDB</csv-db-name>
      <csv-tables>
        <csv-table>
          <schema>../test3/source/Book.csv</schema>
          <instances />
        </csv-table>
        <csv-table>
          <schema>../test3/source/book2.csv</schema>
          <instances />
        </csv-table>
      </csv-tables>
    </csv>
    <inclusions />
    <exclusions />
    <duplications />
    <functionalDependencies />
    <selectionConditions />
    <joinConditions />
  </source>
  <target>
    <type>CSV</type>
    <csv>
      <csv-db-name>TargetDB</csv-db-name>
      <csv-tables>
        <csv-table>
          <schema>../test3/target/customer.csv</schema>
          <instances />
        </csv-table>
      </csv-tables>
    </csv>
    <inclusions />
    <exclusions />
    <duplications />
    <functionalDependencies />
    <selectionConditions />
    <joinConditions />
  </target>
  <correspondences>
    <correspondence>
      <source-paths>
        <source-path>SourceDB.Book.BookTuple.id</source-path>
      </source-paths>
      <target-path>TargetDB.customer.customerTuple.first_name</target-path>
      <transformation-function>SourceDB.Book.BookTuple.id</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths />
      <source-value>"hola"</source-value>
      <target-path>TargetDB.customer.customerTuple.last_name</target-path>
      <transformation-function>"hola"</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths>
        <source-path>SourceDB.book2.book2Tuple.bookid</source-path>
        <source-path>SourceDB.book2.book2Tuple.pubid</source-path>
      </source-paths>
      <target-path>TargetDB.customer.customerTuple.company_name</target-path>
      <transformation-function>append(SourceDB.book2.book2Tuple.bookid, SourceDB.book2.book2Tuple.pubid)</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths>
        <source-path>SourceDB.book2.book2Tuple.bookid</source-path>
        <source-path>SourceDB.book2.book2Tuple.pubid</source-path>
      </source-paths>
      <target-path>TargetDB.customer.customerTuple.address</target-path>
      <transformation-function>append(SourceDB.book2.book2Tuple.bookid, SourceDB.book2.book2Tuple.pubid)</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
  </correspondences>
</mappingtask>

